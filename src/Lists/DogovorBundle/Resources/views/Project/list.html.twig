{% trans_default_domain 'ListsDogovorBundle' %}
<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
    <tr>
        <th>{% trans %}ID{% endtrans %}</th>
        <th>{% trans %}Organization{% endtrans %}</th>
    </tr>
    </thead>
    <tbody>
    {% for project in projects %}
        <tr>
            <td>
                <a class="btn{% if project.statusAccess  != true %} yellow{% elseif  project.isClosed  != true %} blue{% else%} red{% endif %} button_wide" href="{{ path('lists_project_'~project.getDiscr~'_show', { id: project.id } ) }}">
                {{ project.id }}
                </a>
            </td>
            <td>{{ project.organization }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
