{% trans_default_domain 'ListsDogovorBundle' %}
<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
    <tr>
        <th>{% trans %}ID{% endtrans %}</th>
        <th>{% trans %}Handling{% endtrans %}</th>
        {% if access.canEdit %}
        <th>{% trans %}Actions{% endtrans %}</th>
        {% endif %}
    </tr>
    </thead>
    <tbody>
    {% for result in results %}
        <tr>
            <td>{{ result.id }}</td>
            <td>{{ result.handling }}</td>
            {% if access.canEdit %}
            <td>
                <a
                        class="ajax-delete"
                        href="javascript:;"
                        data-params='{"model":"HandlingDogovor","id":{{ result.id }}}'
                        >
                    {% trans %}Delete{% endtrans %}
                </a>
            </td>
            {% endif %}
        </tr>
    {% endfor %}
    </tbody>
</table>
