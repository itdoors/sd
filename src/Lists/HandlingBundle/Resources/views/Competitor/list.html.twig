{% trans_default_domain 'ListsHandlingBundle' %}
<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
    <tr>
        <th>{% trans %}ID{% endtrans %}</th>
        <th>{% trans %}Competitor{% endtrans %}</th>
        <th>{% trans %}Strengths{% endtrans %}</th>
        <th>{% trans %}Weaknesses{% endtrans %}</th>
        <th>{% trans %}End date{% endtrans %}</th>
        <th>{% trans %}Total{% endtrans %}</th>
        <th>{% trans %}Actions{% endtrans %}</th>
    </tr>
    </thead>
    <tbody>
    {% for result in results %}
        <tr>
            <td>{{ result.id }}</td>
            <td>{{ result.competitor }}</td>
            <td>{{ result.Strengths }}</td>
            <td>{{ result.weaknesses }}</td>
            <td>
                {% if result.endDate %}
                    {{ result.endDate|date('d.m.Y') }}
                {% endif %}
            </td>
            <td>{{ result.total }}</td>
            <td>
                <a
                        class="ajax-delete"
                        href="javascript:;"
                        data-params='{"model":"HandlingCompetitor","id":{{ result.id }}}'
                        >
                    {% trans %}Delete{% endtrans %}
                </a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
