<a class="btn green" id="close-add-btn-{{object.project.id}}" href="#" data-toggle="modal">
    {% trans from 'ListsHandlingBundle'%}Close project{% endtrans %}
</a>
{{ ajax_form_render_btn({
   'selector' : '#close-add-btn-'~ object.project.id,
   'formType' : 'closeProjectForm',
   'successFunctions' : {
        'project_edit' : 'ITDoorsAjax.updateList',
        'list_participants' : 'ITDoorsAjax.updateList',
        'list_documents' : 'ITDoorsAjax.updateList'
   },
   'serviceSave' : {
       'alias' : 'lists_handling.service',
       'method' : 'saveCloseProjectForm'
   },
   'defaults' : {
       'projectId' : object.project.id
   },
   'target' : '#form_modal_closeProjectForm'~object.project.id,
   'html' : 'ListsHandlingBundle:Form:closeProjectForm.html.twig'
}) }}
<div id="form_modal_closeProjectForm{{object.project.id}}" class="modal" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{% trans from 'ListsHandlingBundle' %}Close project{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <div data-text="{% trans %}Loading data. Please wait{% endtrans %}"></div>
            </div>
        </div>
    </div>
</div>