<a class="btn green" id="add-docunebt-add-btn" href="#" data-toggle="modal">
    {% trans from 'ListsProjectBundle'%}Add document{% endtrans %}
</a>
{{ ajax_form_render_btn({
    'selector' : '#add-docunebt-add-btn',
    'formType' : 'addDocumentForm',
    'successFunctions' : {
        'list_documents' : 'ITDoorsAjax.updateList'
    },
'serviceDefault' : {
        'alias' : 'lists_handling.service',
        'method' : 'addDocumentFormDefaults'
    },
    'serviceSave' : {
        'alias' : 'lists_handling.service',
        'method' : 'saveAddDocumentForm'
    },
    'defaults' : {
        'project' : object.project.id
    },
    'target' : '#form_modal_addDocumentForm',
    'html' : 'ListsHandlingBundle:Form:addDocumentForm.html.twig'
}) }}

<div id="form_modal_addDocumentForm" class="modal" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{% trans from 'ListsHandlingBundle' %}Add document{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <div data-text="{% trans %}Loading data. Please wait{% endtrans %}"></div>
            </div>
        </div>
    </div>
</div>