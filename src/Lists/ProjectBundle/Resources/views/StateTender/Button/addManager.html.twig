<a class="btn green margin-bottom-20" id="project-add-manager-add-btn" href="#" data-toggle="modal">
    {% trans from 'ListsProjectBundle'%}Add manager{% endtrans %}
</a>
{{ ajax_form_render_btn({
    'selector' : '#project-add-manager-add-btn',
    'formType' : 'addManagerForm',
    'successFunctions' : {
        'managers_list' : 'ITDoorsAjax.updateList'
    },
'   serviceDefault' : {
        'alias' : 'lists_project.service',
        'method' : 'addManagerFormDefaults'
    },
    'serviceSave' : {
        'alias' : 'lists_project.service',
        'method' : 'saveAddManagerForm'
    },
    'defaults' : {
        'project' : object.id
    },
    'target' : '#form_modal_addManagerForm',
    'html' : 'ListsProjectBundle:Form:addManagerForm.html.twig'
}) }}

<div id="form_modal_addManagerForm" class="modal" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{% trans from 'ListsProjectBundle' %}Add manager in project{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <div data-text="{% trans %}Loading data. Please wait{% endtrans %}"></div>
            </div>
        </div>
    </div>
</div>