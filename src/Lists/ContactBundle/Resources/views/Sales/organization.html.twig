    {% trans_default_domain 'ListsContactBundle' %}

    <script type="text/javascript">
        $(document).ready(function()
        {
            $.fn.editable.defaults.inputclass = 'form-control';
            $.fn.editable.defaults.emptytext = '{% trans %}Enter value{% endtrans %}';

            $('#organization-contact-list').editable({
                url: '{{ path('sd_common_ajax_model_contact_save') }}',
                selector: '.sd-contact-editable-text'
            });

            $('#organization-contact-list').editable({
                url: '{{ path('sd_common_ajax_model_contact_save') }}',
                selector: '.sd-contact-editable-phone',
                select2: {
                    width: '200px',
                    minimumInputLength: 2,
                    allowClear: true,
                    ajax: {
                        url: '{{ path('sd_common_ajax_contact_phone') }}',
                        dataType: 'json',
                        data: function (term, page) {

                            var queryData = {
                                query: term
                            };

                            /*if (self.data('organization-id'))
                            {
                                queryData.organizationId = self.data('organization-id')
                            }*/

                            return queryData;
                        },
                        results: function (data, page) {
                            return {
                                results: data
                            };
                        }
                    }
                }

            });

            $.fn.modal.Constructor.prototype.enforceFocus = function() {};

            $('.sd-contact-birthday').live('mouseover', function(e){

                e.preventDefault();

                $(this).on('shown', function(e, editable) {

                    var input = editable.input.$input;

                    input.attr('readonly', 'readonly');

                    input.datepicker({
                        rtl: App.isRTL(),
                        autoclose: true,
                        format: "dd.mm.yyyy",
                        pickerPosition: (App.isRTL() ? "bottom-right" : "bottom-left"),
                        language: 'ru',
                        startView: 'decade',
                        endDate: new Date(),
                        weekStart: 1
                    });
                });
            })
        });
    </script>

    <div id="form_modal_contact" class="modal fade" role="basic" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                    <h4 class="modal-title">{% trans %}Contact edit{% endtrans %}</h4>
                </div>
                <div class="modal-body">
                    <div id="modelContactFormTpl" data-text="{% trans %}Loading data. Please wait{% endtrans %}">
                    </div>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
    </div>

    {% if organizationId %}
    {% else %}
        <div class="navigation">
            {{ knp_pagination_render(pagination) }}
        </div>
    {% endif %}

	<div class="table-scrollable" id="organization-contact-list">
        <table class="table table-striped table-hover table-bordered">
            <thead>
            <tr>
                <th>#</th>
                {% if organizationId %}
                {% else %}
                    <th>{% trans %}Organization{% endtrans %}</th>
                {% endif %}
                <th>{% trans %}FirstName{% endtrans %}</th>
                <th>{% trans %}LastName{% endtrans %}</th>
                <th>{% trans %}SurName{% endtrans %}</th>
                <th>{% trans %}Position{% endtrans %}</th>
                <th>{% trans %}Phone{% endtrans %}</th>
                <th>{% trans %}Mobile{% endtrans %}</th>
                <th>{% trans %}Email{% endtrans %}</th>
                <th>{% trans %}Birthday{% endtrans %}</th>
                <th>{% trans %}Contact type{% endtrans %}</th>
                <th>{% trans %}Owner{% endtrans %}</th>
                <th>{% trans %}Creator{% endtrans %}</th>
            </tr>
            </thead>
            <tbody>
            {% for item in pagination %}
            <tr id="ModelContact{{ item.0.id }}"
                data-url="{{ path('lists_' ~ baseRoutePrefix ~ '_contact_ajax_organization_element', {
                    id: item.0.id
                }) }}"
            >
                {{  include(
                    'ListsContactBundle:' ~ baseTemplate ~ ':organizationElement.html.twig',
                        {
                            item: item,
                            baseRoutePrefix: baseRoutePrefix,
                            organizationId: organizationId
                        }
                    )
                }}
            </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
    {% if organizationId %}
    {% else %}
        <div class="navigation">
            {{ knp_pagination_render(pagination) }}
        </div>
    {% endif %}