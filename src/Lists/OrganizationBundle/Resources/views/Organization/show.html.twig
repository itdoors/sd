{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Organization</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Mpk</th>
                <td>{{ entity.mpk }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Contacts</th>
                <td>{{ entity.contacts }}</td>
            </tr>
            <tr>
                <th>Shortname</th>
                <td>{{ entity.shortname }}</td>
            </tr>
            <tr>
                <th>Issmeta</th>
                <td>{{ entity.isSmeta }}</td>
            </tr>
            <tr>
                <th>Mailingaddress</th>
                <td>{{ entity.mailingAddress }}</td>
            </tr>
            <tr>
                <th>Rs</th>
                <td>{{ entity.rs }}</td>
            </tr>
            <tr>
                <th>Edrpou</th>
                <td>{{ entity.edrpou }}</td>
            </tr>
            <tr>
                <th>Inn</th>
                <td>{{ entity.inn }}</td>
            </tr>
            <tr>
                <th>Certificate</th>
                <td>{{ entity.certificate }}</td>
            </tr>
            <tr>
                <th>Shortdescription</th>
                <td>{{ entity.shortDescription }}</td>
            </tr>
            <tr>
                <th>Site</th>
                <td>{{ entity.site }}</td>
            </tr>
            <tr>
                <th>Organization_type_id</th>
                <td>{{ entity.organizationtypeid }}</td>
            </tr>
            <tr>
                <th>City_id</th>
                <td>{{ entity.cityid }}</td>
            </tr>
            <tr>
                <th>Scope_id</th>
                <td>{{ entity.scopeid }}</td>
            </tr>
            <tr>
                <th>Createdatetime</th>
                <td>{{ entity.createdatetime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Physicaladdress</th>
                <td>{{ entity.physicalAddress }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ entity.phone }}</td>
            </tr>
            <tr>
                <th>Group_id</th>
                <td>{{ entity.groupid }}</td>
            </tr>
            <tr>
                <th>Parent_id</th>
                <td>{{ entity.parentid }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('organization') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('organization_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
