{#{{ base_table_render(
    serviceCovers,
    options,
    ajax_namespace_organziation_service_cover ~ organizationId,
    'service-cover-table'
) }}#}
{% trans_default_domain 'ListsOrganizationBundle' %}
<table class="table table-striped table-bordered table-advance table-hover">
    <thead>
        <tr>
            <th>{% trans %}ID{% endtrans %} </th>
            <th>{% trans %}serviceName{% endtrans %} </th>
            <th>{% trans %}Is interested{% endtrans %} </th>
            <th>{% trans %}Is working{% endtrans %} </th>
            <th>{% trans %}Evaluation{% endtrans %} </th>
            <th>{% trans %}Competitor{% endtrans %} </th>
            <th>{% trans %}End date{% endtrans %} </th>
            <th>{% trans %}Responsible{% endtrans %} </th>
            <th>{% trans %}Description{% endtrans %} </th>
            <th>{% trans %}Actions{% endtrans %} </th>
        </tr>
    </thead>
    <tbody>
    {% for item in serviceCovers %}
        <tr {% if loop.index is odd %}class="color"{% endif %}>
            <td>
                {{ item.id|e }}
            </td>
            <td>{{ item.serviceName|e }}</td>
            <td>
                {% if item.isInterested %}
                    {% set isInterested ='Yes' %}
                {% else %}
                    {% set isInterested ='No' %}
                {% endif %}
                {{ isInterested|trans({}, 'messages') }}
            </td>
            <td>
                {% if item.isWorking %}
                    {% set isWorking ='Yes' %}
                {% else %}
                    {% set isWorking ='No' %}
                {% endif %}
                {{ isInterested|trans({}, 'messages') }}
            </td>
            <td>{{ item.evaluation|e }}</td>
            <td>{{ item.competitorName|e }}</td>
            <td>
                {% if item.endDate %}
                    {{ item.endDate|date("d.m.Y") }}
                {% endif %}
            </td>
            <td>{{ item.responsible|e }}</td>
            <td>{{ item.description|e }}</td>
            <td>
                <a  class="ajax-delete btn red"
                    href="javascript:;"
                    data-params='{"model":"OrganizationServiceCover","id":{{ item.id }}}'
                >
                    {% trans from 'messages' %}Delete{% endtrans %}
                </a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>

