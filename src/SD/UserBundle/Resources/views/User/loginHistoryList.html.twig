{% trans_default_domain 'SDUserBundle' %}
<div class="navigation"></div>
<div class="table-scrollable" style="clear: right;">
    <table class="table table-striped table-bordered table-hover dataTable" aria-describedby="datatable_ajax_info">
        <thead>
            <tr role="row" class="heading">
                <th width="10%" class="sorting_disabled" role="columnheader" rowspan="1" colspan="1">{% trans %}â„–{% endtrans %}</th>
                <th width="20%" class="sorting_disabled" role="columnheader" rowspan="1" colspan="1">{% trans %}IP{% endtrans %}</th>
                <th width="35%" class="sorting_disabled" role="columnheader" rowspan="1" colspan="1">{% trans %}LogedIn{% endtrans %}</th>
                <th width="35%" class="sorting_disabled" role="columnheader" tabindex="0" rowspan="1" colspan="1">{% trans %}LogedOut{% endtrans %}</th>
            </tr>
        </thead>
        <tbody role="alert" aria-live="polite" aria-relevant="all">
            {% for key,item in items %}
            <tr class="{{ cycle(['odd', 'even'], item.clientIp) }}">
                <td>{{key+1}}</td>
                {% if currentIp == item.clientIp %}
                    <td>{% trans %}this cumputer{% endtrans %}</td>
                {% else %}
                    <td><span style="color: red">{{item.clientIp}}</span></td>
                {% endif %}
                <td>{{item.logedIn | date('d-m-Y H:i')}}</td>
                <td>{{item.logedOut  is empty ? "" : item.logedOut | date('d-m-Y H:i')}}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>