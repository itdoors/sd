<a class="btn green" id="acceptance-add-btn-{{entity.id}}" href="#" data-toggle="modal">
    {% trans from 'ITDoorsPayMasterBundle' %}Acceptance{% endtrans %}
</a>
{{ ajax_form_render_btn({
   'selector' : '#acceptance-add-btn-'~ entity.id,
   'formType' : 'payMasterAcceptanceForm',
   'successFunctions' : {
       'table_tab' : 'ITDoorsAjax.updateList'
   },
   'serviceDefault' : {
       'alias' : 'it_doors_pay_master.service',
       'method' : 'addPayMasterAcceptanceFormDefaults'
   },
   'serviceSave' : {
       'alias' : 'it_doors_pay_master.service',
       'method' : 'savePayMasterAcceptanceForm'
   },
   'defaults' : {
       'payMasterId' : entity.id
   },
   'target' : '#form_modal_payMasterAcceptance',
   'html' : 'ITDoorsPayMasterBundle:Form:payMasterAcceptanceForm.html.twig'
}) }}

<div id="form_modal_payMasterAcceptance" class="modal" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{% trans %}Acceptance{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <div data-text="{% trans %}Loading data. Please wait{% endtrans %}"></div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
</div>