{% trans_default_domain 'ListsContactBundle' %}

    <div class="navigation right_float">
        {{ knp_pagination_render(pagination) }}
    </div>

<div class="table-scrollable" id="organization-contact-list">
    <table class="table table-striped table-hover table-bordered">
        <thead>
        <tr>
            <th>#</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Mpk{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Organization{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Region{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}City{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Address{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Status{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}StatusDate{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Type{% endtrans %}</th>
            <th>{% trans  from 'ITDoorsOperBundle' %}Description{% endtrans %}</th>
        </tr>
        </thead>
        <tbody>
        {% for item in pagination %}
            <tr id="ModelContact{{ item.id }}">
                {{  include(
                'ITDoorsOperBundle:Parts:departmentElement.html.twig',
                {
                item: item,
                baseRoutePrefix: 0,
                }
                )
                }}
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>

    <div class="navigation right_float">
        {{ knp_pagination_render(pagination) }}
    </div>

