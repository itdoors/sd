{% trans_default_domain 'ITDoorsHistoryBundle' %}
 <a class="btn green" id="history-show-btn" href="#history" data-toggle="modal">
    {% trans %}Show history{% endtrans %}
</a>
    
{{ history_table_render_btn({
    'selector' : '#history-show-btn',
    'service' : {
        'alias' : 'it_doors_history.service',
        'method' : 'getHistory'
    },
    'params' : {
        'modelName' : modelName,
        'modelId' : modelId,
        'showPagination' : showPagination is defined ? showPagination : 'true',
        'countPagination' : countPagination is defined ? countPagination : '10',
        'showFilter' : showFilter is defined ? showFilter : 'false'
    },
    'target' : '#historyBlock',
    'html' : 'ITDoorsHistoryBundle:History:index.html.twig'
}) }}
<style>
    modal-backdrop fade in {
        z-index: 10049 !important
    }
</style>
<div id="history" class="modal fade" role="basic" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">{% trans %}History{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <div id="historyBlock" data-text="{% trans from 'ListsContactBundle' %}Loading data. Please wait{% endtrans %}">
                </div>
            </div>
        </div>
    </div>
</div>