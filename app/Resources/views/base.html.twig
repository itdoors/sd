<!--[if !IE]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
	<meta charset="utf-8" />
	<title>{% block title %}Default title{% endblock %}</title>
	<!--[if IE]>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<![endif]-->
	<meta content="width=device-width, initial-scale=1.0" name="viewport" />
	<meta content="" name="description" />
	<meta content="" name="author" />
	<meta name="MobileOptimized" content="320">
	
	<!-- BEGIN GLOBAL MANDATORY STYLES -->  
	{% block css_global %}        
		<link href="{{ asset('templates/metronic/plugins/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/plugins/uniform/css/uniform.default.css') }}" rel="stylesheet" type="text/css"/>
		<link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&amp;subset=latin,cyrillic' rel='stylesheet' type='text/css'/>
	{% endblock %}	
	<!-- END GLOBAL MANDATORY STYLES -->
	<!-- BEGIN PAGE LEVEL PLUGIN STYLES --> 
	{% block css_page_level_plugin %} 
		<link href="{{ asset('templates/metronic/plugins/gritter/css/jquery.gritter.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css') }}" rel="stylesheet" type="text/css" />
		<link href="{{ asset('templates/metronic/plugins/fullcalendar/fullcalendar/fullcalendar.css') }}" rel="stylesheet" type="text/css"/>
		<!--<link href="{{ asset('templates/metronic/plugins/jqvmap/jqvmap/jqvmap.css') }}" rel="stylesheet" type="text/css"/>-->
		<link href="{{ asset('templates/metronic/plugins/jquery-easy-pie-chart/jquery.easy-pie-chart.css') }}" rel="stylesheet" type="text/css"/>
	{% endblock %}
	<!-- END PAGE LEVEL PLUGIN STYLES -->
	
	<!-- BEGIN THEME STYLES --> 
	{% block css_theme_styles %} 
		<link href="{{ asset('templates/metronic/css/style-metronic.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/css/style.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/css/style-responsive.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/css/plugins.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/css/pages/tasks.css') }}" rel="stylesheet" type="text/css"/>
		<link href="{{ asset('templates/metronic/css/themes/default.css') }}" rel="stylesheet" type="text/css" id="style_color"/>
		<link href="{{ asset('templates/metronic/css/custom.css') }}" rel="stylesheet" type="text/css"/>
	{% endblock %}
	<!-- END THEME STYLES -->
    <script src="{{ asset('templates/metronic/plugins/jquery-1.10.2.min.js') }}" type="text/javascript"></script>
	<link rel="shortcut icon" href="favicon.ico" />

    {% block google_analytics %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-46376346-1', 'griffin.ua');
            ga('send', 'pageview');

        </script>
    {% endblock %}
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed">
	<!-- BEGIN HEADER -->   
	<div class="header navbar navbar-inverse navbar-fixed-top">
		<!-- BEGIN TOP NAVIGATION BAR -->
		<div class="header-inner">
			<!-- BEGIN LOGO -->  
			<a class="navbar-brand" href="index.html">
			<img src="{{ asset('/templates/core/img/logo-small-gray.png') }}" alt="{% trans %}Impel Griffin logotype{% endtrans %}" class="img-responsive" />
			</a>
			<!-- END LOGO -->
			{#% include 'menu_horizonal.html.twig' %#}

		</div>
		<!-- END TOP NAVIGATION BAR -->
	</div>
	<!-- END HEADER -->
	<div class="clearfix"></div>
	<!-- BEGIN CONTAINER -->
	<div class="page-container">
		{% include 'sidebar.html.twig' %} 
		<!-- BEGIN PAGE -->
		<div class="page-content">
			{# {% include 'modal.html.twig' %} #}
			{# {% include 'style_customizer.html.twig' %} #}
			<!-- BEGIN PAGE HEADER-->
			<div class="row">
				<div class="col-md-12">
					<!-- BEGIN PAGE TITLE & BREADCRUMB-->
					{% block page_title %}
					<h3 class="page-title">
						 {% trans %}Default <small>default template</small>{% endtrans %}  
					</h3>
					{% endblock %}
					{% block page_breadcrumbs %}
					<ul class="page-breadcrumb breadcrumb">
						<li>
							<i class="fa fa-home"></i>
							<a href="{{ path('sd_dashboard_homepage') }}">{% trans %}Home{% endtrans %}</a> 
							<i class="fa fa-angle-right"></i>
						</li>
						<li><a href="#">Dashboard</a></li>						
					</ul>
					{% endblock %}
					<!-- END PAGE TITLE & BREADCRUMB-->
				</div>
			</div>
			<!-- END PAGE HEADER-->
			
			{% block body %}{% endblock %}			
			
		</div>
		<!-- END PAGE -->
	</div>
	<!-- END CONTAINER -->
	<!-- BEGIN FOOTER -->
	<div class="footer">
		<div class="footer-inner">
			2013 &copy; ITDoors <a href="http://itdoors.com.ua" target="_blank">www.itdoors.com.ua</a>.
		</div>
		<div class="footer-tools">
			<span class="go-top">
			<i class="fa fa-angle-up"></i>
			</span>
		</div>
	</div>
	<!-- END FOOTER -->
	<!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
