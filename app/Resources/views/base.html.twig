    {% block js_page_init_scripts %}
        <script>
            jQuery(document).ready(function() {
                Metronic.init(); // init metronic core components
                Layout.init(); // init current layout

                SD.init({
                    ajaxFormUrl: '{{ path('sd_common_ajax_form') }}',
                    ajaxDeleteUrl: '{{ path('sd_common_ajax_delete') }}',
                    assetsDir: '{{ asset('') }}'
                });

                ITDoorsAjax.init({
                    assetsDir: '{{ asset('') }}'
                });

                ITDoorsHistory.init();

                ITDoorsSip.init({
                    io: {
                        host: '{{ ami_host }}',
                        port: '{{ ami_port }}'
                    },
                    sip: {
                        host: '{{ sip_host }}',
                        hostProxy: '{{ sip_host_proxy }}',
                        portUdp: '{{ sip_port_udp }}',
                        portWs: '{{ sip_port_ws }}'
                    },
                    audio: {
                        sounds: {
                            ringtoneSrc: '{{ asset('bundles/itdoorssip/sipml/sounds/ringtone.wav')}}',
                            ringbacktoneSrc: '{{ asset('bundles/itdoorssip/sipml/sounds/ringbacktone.wav')}}',
                            dtmfToneSrc: '{{ asset('bundles/itdoorssip/sipml/sounds/dtmf.wav')}}'
                        }
                    }
                });
                if (!sessionStorage.getItem("showed")) {
                    jQuery.ajax({
                        dataType: "json",
                        type: "GET",
                        url: "{{ path('list_article_blog_last') }}",
                        success: function(article) {
                            if (article) {
                                jQuery('#modtitle').html(article.title);
                                jQuery('#moreInfoTpl').html(article.text);
                                jQuery('#form_modal_1').modal('show');
                                jQuery("#showed").click(function() {
                                    sessionStorage.setItem("showed", true);
                                    jQuery.ajax({
                                        type: "POST",
                                        url: "{{ path('list_article_blog') }}" + "/viewed/" + article.id
                                    });
                                    location.reload();
                                });
                            }
                        }
                    });
                }

                //console.log('ITDoorsSip', ITDoorsSip);
                //Index.initJQVMAP(); // init index page's custom scripts
                //Index.initCalendar(); // init index page's custom scripts
                //Index.initCharts(); // init index page's custom scripts
                //Index.initChat();
                //Index.initMiniCharts();
                //Index.initDashboardDaterange();
                //Index.initIntro();
                //Tasks.initDashboardWidget();
            });
        </script>
